{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": "airbnb",
    "parserOptions": {
        "sourceType": "module"
    },
    "globals": {
        "jasmine": true,
        "describe": true,
        "it": true,
        "beforeEach": true,
        "afterEach": true,
        "expect": true,
        "inject": true
    },
    "rules": {
        "linebreak-style": [
            "error",
            "windows"
        ]
    }
}